{
  "name": "vite-boilerplate",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\" \"npm run storybook\"",
    "client:dev": "vite",
    "server:dev": "ts-node-dev --project tsconfig.server.json src/server",
    "server:build": "tsc --project tsconfig.server.json",
    "client:build": "vite build",
    "storybook:build": "storybook build -o dist/storybook",
    "build": "npm run server:build && npm run client:build && npm run storybook:build",
    "serve": "vite preview",
    "prepare": "husky install",
    "test:lint": "tsc && prettier --check . && eslint . && stylelint \"**/*.css\"",
    "storybook": "storybook dev -p 6006",
    "start": "node dist/server.js",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@chromatic-com/storybook": "^3.2.2",
    "@storybook/addon-actions": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.5",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.14",
    "@types/node": "^16.11.4",
    "@types/react": "^17.0.83",
    "@types/react-dom": "^17.0.25",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-loader": "^8.2.3",
    "concurrently": "^6.3.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "http-proxy-middleware": "^2.0.1",
    "husky": "^7.0.4",
    "jest": "^29.7.0",
    "lint-staged": "^11.2.4",
    "prettier": "2.4.1",
    "storybook-builder-vite": "^0.1.2",
    "stylelint": "^14.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^23.0.0",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2",
    "vite": "^4.5.5",
    "whatsapp-chat-parser": "^4.0.2"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@fortawesome/fontawesome-free": "^6.7.1",
    "@mdx-js/react": "^3.1.0",
    "@mui/material": "^5.11.0",
    "@vitejs/plugin-react": "^4.3.4",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fast-marquee": "^1.6.5",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^5.3.0",
    "xregexp": "^5.1.1"
  }
}
